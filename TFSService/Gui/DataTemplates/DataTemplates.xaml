<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:client="clr-namespace:Microsoft.TeamFoundation.WorkItemTracking.Client;assembly=Microsoft.TeamFoundation.WorkItemTracking.Client"
                    xmlns:view="clr-namespace:Gui.View"
                    xmlns:viewModels="clr-namespace:Gui.ViewModels">
    
    <DataTemplate DataType="{x:Type viewModels:WorkItemVm}">
        <view:WorkItemView DataContext="{Binding Item}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:WorkItemSearcher}">
        <view:WorkItemSearcherView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:FirstConnectionViewModel}">
        <view:FirstConnectView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:ChooseTaskViewModel}">
        <view:ChooseTaskView/>
    </DataTemplate>
    
    <!--<DataTemplate DataType="{x:Type viewModels:TextInputVm}">
        <view:TextInputView/>
    </DataTemplate>-->
    
</ResourceDictionary>